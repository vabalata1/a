	local Uptime = 0
	local Uptime2 = "Server Uptime: %d Seconds"
	local Players = "Players: %d"
	local PlayersLabel = Tab:CreateLabel(Players:format(#game.Players:GetPlayers()))
	local UptimeLabel = Tab:CreateLabel(Uptime2:format(Uptime))

